    "pg": "^8.11.3",
    "prisma": "^6.19.0",
    "redis": "^4.6.10",
    "sharp": "^0.33.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
